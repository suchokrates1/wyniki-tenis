# Example environment configuration for wyniki-tenis
# Copy this file to `.env` and adjust values to match your environment.

# Flask application settings
FLASK_APP=main.py
FLASK_ENV=development
PORT=5000

# Database connection string (SQLite file by default)
DATABASE_URL=sqlite:///overlay.db

# Basic authentication protecting the /config panel
CONFIG_AUTH_USERNAME=admin
CONFIG_AUTH_PASSWORD=change-me

# Logging level for the application (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
